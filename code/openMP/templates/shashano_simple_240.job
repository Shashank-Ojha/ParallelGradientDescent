# Move to my $SCRATCH directory.
cd $SCRATCH

APP_NAME=gd # CHANGE THIS TO THE NAME OF YOUR PROGRAM

# Set this to the important directory.
execdir=/home/shashano/ParallelGradientDescent/code/openMP # The directory that contains your wire routing program
exe=$APP_NAME # The name of your wire routing program
input=simple.txt # The name of the input file
inputname=`basename $input`
args="-f $inputname -n 240"
inputdir=$execdir/data # The directory for test cases

echo $inputname
# Copy executable to $SCRATCH.
cp $execdir/$exe $exe
cp $inputdir/$input $inputname

# Run my executable
perf stat -e cache-misses ./$exe $args
